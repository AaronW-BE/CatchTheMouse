.container {
  height: 100vh;
  position: relative;
}
.wrap {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.list {
  display: flex;
  align-items: center;
  /*margin-top: -10rpx;*/
}
.even {
  margin-left: 35rpx;
}
.item {
  margin-left: 15rpx;
  width: 70rpx;
  height: 70rpx;
}
.item image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.model {
  position: absolute;
  top: 0;

  /*width: 100%;*/
  /*height: 100vh;*/
